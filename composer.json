{
  "name": "pimenvibritania/doo-tools",
  "description": "DOOgether tools",
  "license": "MIT",
  "homepage": "https://github.com/pimenvibritania/doo-tools",
  "keywords": ["cli","command-line", "demo"],
  "autoload": {
    "psr-4": {
      "Dootools\\": "src/"
    }
  },
  "require": {
    "minicli/minicli": "^3.0",
    "ext-json": "*"
  },
  "bin": [
    "bin/doo-tools"
  ],
  "config": {
    "bin-dir": "./"
  },
  "scripts": {
    "post-install-cmd": [
      "php -r \"copy('vendor/bin/doo-tools', './');\""
    ]
  }
}
